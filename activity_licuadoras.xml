<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#A2E4D8">

    <EditText
        android:id="@+id/searchEditText"
        android:layout_width="254dp"
        android:layout_height="56dp"
        android:layout_marginEnd="16dp"
        android:layout_toStartOf="@+id/loginButton"
        android:background="@drawable/edittext_background"
        android:hint="Buscar..." />

    <Button
        android:id="@+id/createAccountButton"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="9dp"
        android:background="@drawable/ic_logo_crearcuenta" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/createAccountButton"
        android:layout_marginTop="6dp"
        android:layout_marginEnd="-54dp"
        android:layout_toStartOf="@id/createAccountButton"
        android:text="Crear Cuenta"
        android:textColor="#000000"
        android:textSize="10sp" />

    <Button
        android:id="@+id/loginButton"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_toStartOf="@id/createAccountButton"
        android:layout_marginEnd="20dp"
        android:background="@drawable/ic_logo_iniciarsesion" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/loginButton"
        android:layout_marginTop="6dp"
        android:layout_marginEnd="-58dp"
        android:layout_toStartOf="@id/loginButton"
        android:text="Iniciar Sesión"
        android:textColor="#000000"
        android:textSize="10sp" />




    <View
        android:layout_width="48dp"
        android:layout_height="2dp"
        android:layout_centerHorizontal="true"
        android:layout_toStartOf="@id/loginButton"
        android:layout_toEndOf="@id/createAccountButton"
        android:background="#000000" />


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/searchEditText"
        android:layout_marginTop="20dp"
        android:background="#000000">

        <TextView
            android:id="@+id/productsTitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="1dp"
            android:text="Licuadoras"
            android:textColor="#E3F2FD"
            android:textSize="18sp" />



    </RelativeLayout>




    <ImageView
        android:id="@+id/imageView_licuadora1"
        android:layout_width="139dp"
        android:layout_height="134dp"
        android:layout_alignParentStart="true"
        android:layout_marginStart="14dp"
        android:layout_marginTop="150dp"
        android:src="@drawable/ic_licuadora1" />

    <TextView
        android:id="@+id/textoDebajoImagen"
        android:layout_width="145dp"
        android:layout_height="49dp"
        android:layout_below="@id/imageView_licuadora1"
        android:layout_alignParentStart="true"
        android:layout_marginStart="19dp"
        android:layout_marginTop="-182dp"
        android:text="Licuadora IMUSA Infiny Force 10 Velocidades Negro,10 Funciones+Pulso garantizando,6 Cuchillas en acero inoxidable,Potencia de 600 Watts,
        $149.900"
        android:textColor="@android:color/black"
        android:textSize="8dp" />

    <Button
        android:id="@+id/btnAgregarAlCarrito"
        android:layout_width="118dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/imageView_licuadora1"
        android:layout_alignParentStart="true"
        android:layout_marginStart="26dp"
        android:layout_marginTop="0dp"
        android:text="Comprar" />



    <ImageView
        android:id="@+id/imageView_licuadora2"

        android:layout_width="153dp"
        android:layout_height="134dp"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="150dp"
        android:layout_marginEnd="30dp"
        android:src="@drawable/ic_licuadora2" />

    <TextView
        android:id="@+id/textoDebajoImagen2"
        android:layout_width="181dp"
        android:layout_height="50dp"
        android:layout_below="@id/imageView_licuadora2"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="-185dp"
        android:layout_marginEnd="3dp"
        android:text="Licuadora IMUSA Powermix Glass 8 Velocidades Gris,8 Velocidades + Función pulso + Autolimpieza,Potencia de 550 Watts,Sistema de acople hecho en Carbon Drive o Policarbonato, 2 veces más resistente.
        $144.900"
        android:textColor="@android:color/black"
        android:textSize="8dp" />




    <Button
        android:id="@+id/btnAgregarAlCarrito2"
        android:layout_width="118dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/imageView_licuadora2"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="46dp"
        android:text="comprar" />

    <ImageView
        android:id="@+id/imageView_licuadora3"
        android:layout_width="139dp"
        android:layout_height="134dp"
        android:layout_alignParentStart="true"
        android:layout_marginStart="15dp"
        android:layout_marginTop="400dp"
        android:src="@drawable/ic_licuadora3" />



    <TextView
        android:id="@+id/textoDebajoImagen3"
        android:layout_width="185dp"
        android:layout_height="32dp"
        android:layout_below="@id/imageView_licuadora3"
        android:layout_alignParentStart="true"
        android:layout_marginStart="14dp"
        android:layout_marginTop="-172dp"
        android:text="Licuadora IMUSA Infiny Force 12V Negra,12 Funciones + Pulso garantizando,6 Cuchillas en acero inoxidable.  $ 179.900"
        android:textColor="@android:color/black"
        android:textSize="8dp" />

    <Button
        android:id="@+id/btnAgregarAlCarrito3"
        android:layout_width="118dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/imageView_licuadora3"
        android:layout_alignParentStart="true"
        android:layout_marginStart="25dp"
        android:layout_marginTop="0dp"
        android:text="comprar" />

    <ImageView
        android:id="@+id/imageView_licuadora4"

        android:layout_width="153dp"
        android:layout_height="134dp"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="400dp"
        android:layout_marginEnd="29dp"
        android:src="@drawable/ic_licuadora4" />

    <TextView
        android:id="@+id/textoDebajoImagen4"
        android:layout_width="186dp"
        android:layout_height="33dp"
        android:layout_below="@id/imageView_licuadora4"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="-175dp"
        android:layout_marginEnd="7dp"
        android:text="Licuadora IMUSA Powermix Glass 2 Velocidades Blanca,2 Velocidades + Función pulso,Potencia de 550 Watts.

$ 119.900"
        android:textColor="@android:color/black"
        android:textSize="8dp" />

    <Button
        android:id="@+id/btnAgregarAlCarrito4"
        android:layout_width="118dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/imageView_licuadora4"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="46dp"
        android:text="comprar" />

    <TextView
        android:id="@+id/serviceTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="600dp"
        android:text="Servicio al cliente"
        android:textColor="#000000"
        android:textSize="18sp" />

    <Button
        android:id="@+id/whatsappButton"
        android:layout_width="56dp"
        android:layout_height="48dp"
        android:layout_marginTop="640dp"
        android:background="@drawable/ic_logo_whatsapp"
        android:contentDescription="Logo de WhatsApp"
        android:onClick="onWhatsAppButtonClick" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/loginButton"
        android:layout_marginTop="613dp"
        android:layout_marginEnd="147dp"
        android:layout_toStartOf="@id/loginButton"
        android:text="Atención al cliente"
        android:textColor="#000000"
        android:textSize="10sp" />

    <Button
        android:id="@+id/RastreoButton"
        android:layout_width="38dp"
        android:layout_height="48dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="640dp"
        android:background="@drawable/ic_logo_rastreodecompra"
        android:contentDescription="Rastreodecompra"
        android:onClick="onWhatsAppButtonClick" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/loginButton"
        android:layout_marginTop="613dp"
        android:layout_marginEnd="-46dp"
        android:layout_toStartOf="@id/loginButton"
        android:text="Rastrea tu pedido"
        android:textColor="#000000"
        android:textSize="10sp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/RastreoButton"
        android:background="#000000"
        android:padding="16dp"/>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/searchEditText"
        android:layout_marginTop="20dp"
        android:background="#000000"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="690dp"
        android:text="@ABACOMAX 2023 El confort que mereces, en cada momento que vivimos."
        android:textColor="#E3F2FD"
        android:textSize="12dp" />


</RelativeLayout>
